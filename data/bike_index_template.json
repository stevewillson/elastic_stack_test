{
    "index_patterns": ["bike*"],
    "version": 50003,
    "order" : 3,
    "settings":{
        "number_of_replicas": "0",
        "number_of_shards": "1",
        "index.refresh_interval":"30s"
    },
    "mappings": {
        "doc" : {
            "properties": {
                "@timestamp": {
                    "type": "date"
                },
                "@version": {
                    "type": "text",
                    "fields": {
                        "keyword": {
                            "type": "keyword",
                            "ignore_above": 256
                        }
                    }
                },
                "trip_id": { "type": "integer" },
                "year": { "type": "integer" },
                "month": { "type": "integer" },
                "week": { "type": "integer" },
                "day": { "type": "integer" },
                "hour": { "type": "integer" },
                "usertype": { "type": "text" },
                "gender": { "type": "text" },
                "starttime": { "type": "date_time" },
                "stoptime": { "type": "date_time" },
                "tripduration": { "type": "float" },
                "temperature": { "type": "float" },
                "events": { "type": "text" },
                "from_station_id":  { "type": "integer" },
                "from_station_name": { "type": "text" },

                "location_start": { "type": "geo_point" },

                "dpcapacity_start": { "type": "float" },
                "to_station_id":  { "type": "integer" },
                "to_station_name": { "type": "text" },

                "location_end": { "type": "geo_point" },

                "dpcapacity_end": { "type": "float" }
            }
        }
    }
}

