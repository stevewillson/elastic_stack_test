{
  "elastic-health": {
    "order": 3,
    "version": 50003,
    "index_patterns": [
      "elasticsearch-index*"
    ],
    "settings": {
      "index": {
        "number_of_shards": "8",
        "number_of_replicas": "0",
        "refresh_interval": "30s"
      }
    },
    "mappings": {
      "doc": {
        "properties": {
          "@timestamp": {
            "type": "date"
          },
          "@version": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "docs_count": {
            "type": "long"
          },
          "docs_deleted": {
            "type": "long"
          },
          "health": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "heap_percent": {
            "type": "long"
          },
          "host": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "index": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "logstash_heap": {
            "type": "long"
          },
          "logstash_time": {
            "type": "float"
          },
          "message": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "path": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "primary_shards": {
            "type": "long"
          },
          "ram_percent": {
            "type": "long"
          },
          "replica_shards": {
            "type": "long"
          },
          "status": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "store_size": {
            "type": "long"
          },
          "store_size_per_shard": {
            "type": "long"
          },
          "index_memory_size": {
            "type": "long"
          },
          "tags": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "uuid": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "queue_push_duration_in_millis": {
            "type": "long"
          },
          "duration_in_millis": {
            "type": "long"
          },
          "out": {
            "type": "long"
          },
          "dropped_events": {
            "type": "long"
          },
          "filtered": {
            "type": "long"
          },
          "in": {
            "type": "long"
          },
          "syslog_ng_sensor": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "destination": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "syslog_ng_protocol": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "syslog_action": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "packets": {
            "type": "long"
          }
        }
      }
    }
  }
}
